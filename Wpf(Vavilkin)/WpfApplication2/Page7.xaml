<Page x:Class="WpfApplication2.Page7"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApplication2"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="1400"
      Title="Page7" Background="#FFF97F7F">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Root.jpg" Stretch="Fill"/>
        </Grid.Background>
        <TextBox x:Name="DeleteEditBox" HorizontalAlignment="Left" Height="397" Margin="50,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="1296" Background="{x:Null}" BorderBrush="{x:Null}" FontSize="20"/>
        <Button x:Name="DeleteEditButton" Click="DeleteEditButton_Click" Content="Изменить маршруты" HorizontalAlignment="Left" Margin="983,446,0,0" VerticalAlignment="Top" Width="363" Height="76" Background="{x:Null}" BorderBrush="{x:Null}" FontSize="24">
            <Button.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="#FFFD0404" Offset="1"/>
                </LinearGradientBrush>
            </Button.Foreground>
        </Button>
    </Grid>
</Page>
